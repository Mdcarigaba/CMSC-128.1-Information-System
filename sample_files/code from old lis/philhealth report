<script type="text/javascript">

$( document ).ready(function() {

              $('#preloader').css('display','none'); 
              $('#side-bar-reports').addClass('active'); 
              $('#sidebarToggle')[0].click();
              document.title = "Reports";
              
   
    $('#dttable').DataTable({

       "order": [[ 1, "asc" ]],
      aLengthMenu: [
          [10,25, 50, 100, 500,1000,2000,3000,4000, -1],
          [10,25, 50, 100 , 500 ,1000,2000,3000,4000, "ALL"]
      ],     
      columnDefs: [ {"width": "12%", "targets": 1}, {
              orderable: false,
              targets:   0,
             
              "width": "5%",
          }
          ],
    });
    
  
});

$('#maincheckbox').click (function () {
     var checkedStatus = this.checked;
    
        $('.checkboxes').prop('checked', checkedStatus);
     
});

function exportToExcel(){
  $('#preloader').css('display','block'); 
  return  true;
}

function generateList(){
  var fr = $("#date_from").val();
  var to = $("#date_to").val();
  if(fr != '' && to != ''){
    
    $('#preloader').css('display','block'); 
    return  true;
  }else{
    $('#alert-empty-fields').css('display','block'); 
    return false;
  }
  
}


  </script>

